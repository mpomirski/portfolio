{
  "extends": [
    "next/core-web-vitals",
    "next",
    "eslint:recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "singleQuote": true,
        "semi": true,
        "tabWidth": 2,
        "trailingComma": "all"
      }
    ],
    "no-console": "error",
    "no-unused-vars": "warn"
  }
}
