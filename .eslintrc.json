{
  "extends": ["next/core-web-vitals", "next", "eslint:recommended"],
  "plugins": ["prettier", "@typescript-eslint"],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "singleQuote": true,
        "semi": true,
        "tabWidth": 2,
        "trailingComma": "all"
      }
    ],
    "no-console": "error",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  }
}
